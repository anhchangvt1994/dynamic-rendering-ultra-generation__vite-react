{
  "name": "vite-project-react",
  "private": true,
  "version": "0.0.0",
  "root": true,
  "scripts": {
    "start": "npm run server-build && ./node_modules/.bin/cross-env ENV=production node -r sucrase/register server/dist/index.uws.js",
    "start:worker": "npm run server-build && ./node_modules/.bin/cross-env ENV=production node server/dist/index.uws.worker.js",
    "start:pm2": "npm run server-build && ./node_modules/.bin/cross-env ENV=production npm run pm2-puppeteer-ssr:uws",
    "start:pm2:worker": "npm run server-build && ./node_modules/.bin/cross-env ENV=production npm run pm2-puppeteer-ssr:uws:worker",
    "dev": "cross-env ENV=development npm run start-dev",
    "dev:fastify": "cross-env ENV=development npm run start-dev:fastify",
    "dev:uws": "cross-env ENV=development npm run start-dev:uws",
    "build": "./node_modules/.bin/tsc -b && ./node_modules/.bin/eslint src/**/*.{t,j}s{,x} --no-error-on-unmatched-pattern --quiet && node -r sucrase/register server/src/utils/BeforeBuildHandler.ts && ./node_modules/.bin/vite build && node -r sucrase/register server/src/utils/AfterBuildHandler.ts",
    "build:esm": "cross-env ESM=true ./node_modules/.bin/vite build && ./node_modules/.bin/tsc",
    "preview": "cross-env MODE=development ENV=production npm run start-preview",
    "preview:fastify": "cross-env MODE=development ENV=production npm run start-preview:fastify",
    "preview:uws": "cross-env MODE=development ENV=production npm run start-preview:uws",
    "start-dev": "cross-env node -r sucrase/register server/src/index.ts",
    "start-dev:fastify": "cross-env node -r sucrase/register server/src/index.fastify.ts",
    "start-dev:uws": "cross-env node -r sucrase/register server/src/index.uws.ts",
    "start-preview": "cross-env node -r sucrase/register server/src/index.ts",
    "start-preview:fastify": "cross-env node -r sucrase/register server/src/index.fastify.ts",
    "start-preview:uws": "cross-env node -r sucrase/register server/src/index.uws.ts",
    "pm2-puppeteer-ssr": "node --require sucrase/register server/src/puppeteer-ssr/pm2.ts",
    "pm2-puppeteer-ssr:fastify": "node --require sucrase/register server/src/puppeteer-ssr/pm2.fastify.ts",
    "pm2-puppeteer-ssr:uws": "node --require sucrase/register server/src/puppeteer-ssr/pm2.uws.ts",
    "pm2-puppeteer-ssr:uws:worker": "node --require sucrase/register server/src/puppeteer-ssr/pm2.uws.worker.ts",
    "start-puppeteer-ssr": "cross-env ./node_modules/.bin/pm2 -i 1 --silent start npm --name \"start-puppeteer-ssr\" -- run pm2-puppeteer-ssr",
    "prettier": "./node_modules/.bin/tsc --noEmit && prettier src/**/*.{t,j}s{,x} --no-error-on-unmatched-pattern --check",
    "lint": "./node_modules/.bin/tsc -b && ./node_modules/.bin/eslint src/**/*.{t,j}s{,x} --no-error-on-unmatched-pattern --no-warn-ignored",
    "lint:fix": "npm run lint -- --fix",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "server-build": "node server/server-build.js",
    "vercel-build": "yarn build",
    "pre-commit": "npm run server-build && git add ./server/dist & ./node_modules/.bin/tsc && lint-staged --allow-empty",
    "prepare": "git config core.autocrlf false && npx husky",
    "preinstall": "node server/src/utils/GenerateServerInfo.js"
  },
  "engines": {
    "node": ">=18.18.x"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "dependencies": {
    "polished": "^4.2.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^7.0.0",
    "styled-components": "^6.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "build-tool-packages": "file:./config/build-tool-packages",
    "commit-packages": "file:./config/commit-packages",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^15.14.0",
    "lint-and-format-packages": "file:./config/lint-and-format-packages",
    "server": "file:./server",
    "typescript-eslint": "^8.21.0"
  },
  "prettier": "./config/prettier.config.cjs",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "npm run prettier:fix && npm run lint"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "feat",
          "style",
          "test",
          "fix",
          "docs",
          "release",
          "update",
          "perf",
          "chore"
        ]
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-git",
      "czConfig": "./config/cz.config.cjs"
    }
  },
  "packageManager": "yarn@1.22.22"
}
